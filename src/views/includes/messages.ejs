<% if(errors.length) {%>
<div class="row">
    <div class="col my-3">
        <div class="alert alert-danger"> 
            <% errors.forEach(erro => { %>
                <%= erro %> <br>    
            <% }) %>
        </div>
    </div>
</div>
<% } %>

<% if(sucess.length > 0) {%>
    <div class="row">
        <div class="col my-3">
            <div class="alert alert-success" role="alert"> 
                <% sucess.forEach(success => { %>
                    <%= sucess %> <br>    
                <% }) %>
            </div>
        </div>
    </div>
    <% } %>